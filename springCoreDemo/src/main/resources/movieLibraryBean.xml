<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->

	<!-- Bean for MovieLibrary with a list of movies -->
	<bean id="movieLibrary" class="com.cgi.springCoreDemo.service.MovieLibrary">
		<property name="movies">
			<list>
				<value>Pursuit of Happyness</value>
				<value>Interstellar</value>
				<value>Sholey</value>
				<value>The Shawshank Redemption</value>
			</list>
		</property>
	</bean>
</beans>
